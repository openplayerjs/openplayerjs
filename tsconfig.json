{
  "buildOnSave": false,
  "compileOnSave": false,
  "compilerOptions": {
      "lib": [
        "DOM",
        "DOM.Iterable",
        "ESNext"
      ],
      "allowSyntheticDefaultImports": true,
      "typeRoots": ["./node_modules/@types"],
      "allowJs": false,
      "target": "es6",
      "module": "ESNext",
      "noImplicitAny": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "removeComments": true,
      "preserveConstEnums": true,
      "sourceMap": false,
      "skipLibCheck": true,
      "strict": true,
      "moduleResolution": "Node",
      "resolveJsonModule": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "esModuleInterop": true,
      "isolatedModules": true,
      "strictPropertyInitialization": false,
      "types": ["cypress", "node"]
  },
  "include": [
    "./src",
    "./_tests_"
  ],
  "exclude": [
    "cypress.config.ts",
    "cypress",
    "node_modules",
    "dist",
    "test"
  ],
}